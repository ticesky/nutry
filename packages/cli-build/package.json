{
    "name": "@nutry/cli-build",
    "version": "1.0.0-alpha.0",
    "license": "ISC",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "tsc -b",
        "clean": "tsc -b --clean && pnpm run clean:extra",
        "clean:extra": "rm -rf dist",
        "lint": "eslint --max-warnings=0 src"
    },
    "dependencies": {
        "@nutry/config-lint": "workspace:*",
        "@nutry/config-webpack": "workspace:*",
        "@nutry/core": "workspace:*",
        "@nutry/settings": "workspace:*",
        "@nutry/utils-build": "workspace:*",
        "kolorist": "^1.8.0",
        "matcher": "^6.0.0",
        "pretty-bytes": "^7.1.0",
        "tty-table": "^5.0.0",
        "webpack-bundle-analyzer": "^5.2.0"
    }
}